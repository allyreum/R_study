select stamp, count(*) as count_bill
from
(select
  case when total_bill >= 25 then 2
    when total_bill >= 15 then 1
    else 0 
  end as stamp
from tips
) a
group by stamp
order by stamp

--
SELECT 
    CASE 
        WHEN total_bill >= 25 THEN 2
        WHEN total_bill >= 15 THEN 1
        ELSE 0
    END AS stamp,
    COUNT(*)
FROM tips
GROUP BY stamp
ORDER BY stamp;
